{
  "patterns": [
    {
      "id": "extract-validate-enrich",
      "name": "Extract-Validate-Enrich Pipeline",
      "description": "Standard document processing pipeline for extracting, validating, and enriching data",
      "sequence": ["document-extractor", "data-validator", "enrichment-agent"],
      "useCases": ["invoice-processing", "form-intake", "document-onboarding", "data-migration"],
      "domains": ["all"],
      "estimatedDuration": "10-20 seconds",
      "humanTouchpoints": []
    },
    {
      "id": "classify-route-process",
      "name": "Classify-Route-Process Pipeline",
      "description": "Intelligent routing based on classification for processing",
      "sequence": ["classifier-agent", "router-agent"],
      "useCases": ["email-triage", "ticket-routing", "work-distribution"],
      "domains": ["all"],
      "estimatedDuration": "2-5 seconds",
      "humanTouchpoints": []
    },
    {
      "id": "validate-approve-execute",
      "name": "Validate-Approve-Execute Pipeline",
      "description": "Validation followed by human approval before execution",
      "sequence": ["data-validator", "compliance-checker", "approval-agent"],
      "useCases": ["payment-processing", "trade-authorization", "policy-changes"],
      "domains": ["finance", "trading"],
      "estimatedDuration": "varies (human approval)",
      "humanTouchpoints": ["approval"]
    },
    {
      "id": "corporate-action-processing",
      "name": "Corporate Action Processing Pipeline",
      "description": "End-to-end corporate action event processing",
      "sequence": ["document-extractor", "data-validator", "enrichment-agent", "classifier-agent", "notification-agent", "election-processor", "position-keeper"],
      "useCases": ["dividend-processing", "merger-processing", "stock-split", "rights-issue"],
      "domains": ["corporate-actions"],
      "estimatedDuration": "1-5 minutes",
      "humanTouchpoints": ["complex-election-review", "exception-handling"]
    },
    {
      "id": "kyc-onboarding",
      "name": "KYC Onboarding Pipeline",
      "description": "Customer onboarding with KYC/AML checks",
      "sequence": ["document-extractor", "data-validator", "kyc-screener", "compliance-checker", "approval-agent", "notification-agent"],
      "useCases": ["customer-onboarding", "periodic-review", "enhanced-due-diligence"],
      "domains": ["kyc", "aml", "onboarding"],
      "estimatedDuration": "5-30 minutes",
      "humanTouchpoints": ["screening-review", "approval", "exception-handling"]
    },
    {
      "id": "trade-settlement",
      "name": "Trade Settlement Pipeline",
      "description": "Trade matching through settlement",
      "sequence": ["trade-matcher", "reconciliation-agent", "compliance-checker", "settlement-instructor", "position-keeper", "notification-agent"],
      "useCases": ["equity-settlement", "fixed-income-settlement", "fx-settlement"],
      "domains": ["trading", "settlement"],
      "estimatedDuration": "30 seconds - 5 minutes",
      "humanTouchpoints": ["break-resolution", "high-value-review"]
    },
    {
      "id": "reconciliation-workflow",
      "name": "Reconciliation Workflow",
      "description": "Data reconciliation with break handling",
      "sequence": ["reconciliation-agent", "exception-handler", "notification-agent", "report-generator"],
      "useCases": ["position-reconciliation", "cash-reconciliation", "transaction-reconciliation"],
      "domains": ["finance", "trading", "accounting"],
      "estimatedDuration": "1-10 minutes",
      "humanTouchpoints": ["break-investigation", "approval"]
    },
    {
      "id": "exception-escalation",
      "name": "Exception Escalation Pattern",
      "description": "Handle exceptions with escalation to humans",
      "sequence": ["exception-handler", "classifier-agent", "router-agent", "notification-agent", "approval-agent"],
      "useCases": ["error-handling", "escalation-management", "sla-breach"],
      "domains": ["all"],
      "estimatedDuration": "varies",
      "humanTouchpoints": ["investigation", "resolution-approval"]
    }
  ]
}
