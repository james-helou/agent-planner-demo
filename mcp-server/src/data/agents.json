{
  "agents": [
    {
      "id": "document-extractor",
      "name": "Document Extraction Agent",
      "description": "Extracts structured data from documents using OCR and NLP",
      "capabilities": ["pdf-parsing", "ocr", "field-extraction", "table-extraction"],
      "inputs": ["pdf", "image", "email", "scanned-document"],
      "outputs": ["structured-data", "key-value-pairs", "extracted-tables"],
      "commonPartners": ["data-validator", "enrichment-agent", "classifier-agent"],
      "domains": ["finance", "legal", "healthcare", "insurance"],
      "estimatedLatency": "2-5 seconds",
      "requiresHumanReview": false
    },
    {
      "id": "data-validator",
      "name": "Data Validation Agent",
      "description": "Validates data against schemas, business rules, and historical patterns",
      "capabilities": ["schema-validation", "rule-checking", "anomaly-detection", "cross-reference-validation"],
      "inputs": ["structured-data", "json", "csv"],
      "outputs": ["validated-data", "validation-report", "anomaly-flags"],
      "commonPartners": ["document-extractor", "enrichment-agent", "exception-handler"],
      "domains": ["all"],
      "estimatedLatency": "1-2 seconds",
      "requiresHumanReview": false,
      "humanReviewTriggers": ["high-value-transactions", "anomaly-detected", "rule-violations"]
    },
    {
      "id": "enrichment-agent",
      "name": "Data Enrichment Agent",
      "description": "Enriches data with external sources and reference data",
      "capabilities": ["reference-data-lookup", "api-integration", "data-augmentation", "entity-resolution"],
      "inputs": ["structured-data", "identifiers"],
      "outputs": ["enriched-data", "lookup-results"],
      "commonPartners": ["data-validator", "classifier-agent", "notification-agent"],
      "domains": ["finance", "kyc", "trading"],
      "estimatedLatency": "3-10 seconds",
      "requiresHumanReview": false,
      "integrations": ["bloomberg", "refinitiv", "dtcc", "swift"]
    },
    {
      "id": "classifier-agent",
      "name": "Classification Agent",
      "description": "Classifies and categorizes data using ML models and rules",
      "capabilities": ["document-classification", "intent-detection", "entity-classification", "priority-scoring"],
      "inputs": ["text", "structured-data", "documents"],
      "outputs": ["classification", "confidence-score", "category"],
      "commonPartners": ["document-extractor", "router-agent", "exception-handler"],
      "domains": ["all"],
      "estimatedLatency": "1-3 seconds",
      "requiresHumanReview": false
    },
    {
      "id": "router-agent",
      "name": "Routing Agent",
      "description": "Routes work items to appropriate queues, teams, or downstream systems",
      "capabilities": ["rule-based-routing", "load-balancing", "priority-routing", "sla-management"],
      "inputs": ["work-item", "classification", "metadata"],
      "outputs": ["routing-decision", "assigned-queue", "sla-deadline"],
      "commonPartners": ["classifier-agent", "notification-agent", "exception-handler"],
      "domains": ["all"],
      "estimatedLatency": "<1 second",
      "requiresHumanReview": false
    },
    {
      "id": "notification-agent",
      "name": "Notification Agent",
      "description": "Sends notifications and communications to stakeholders",
      "capabilities": ["email-generation", "alert-sending", "status-updates", "deadline-reminders"],
      "inputs": ["event", "recipients", "template"],
      "outputs": ["notification-sent", "delivery-status"],
      "commonPartners": ["router-agent", "exception-handler", "approval-agent"],
      "domains": ["all"],
      "estimatedLatency": "1-2 seconds",
      "requiresHumanReview": false,
      "channels": ["email", "slack", "teams", "sms"]
    },
    {
      "id": "approval-agent",
      "name": "Approval Workflow Agent",
      "description": "Manages approval workflows with escalation and delegation",
      "capabilities": ["approval-routing", "escalation", "delegation", "deadline-tracking"],
      "inputs": ["approval-request", "approvers", "rules"],
      "outputs": ["approval-status", "decision", "audit-trail"],
      "commonPartners": ["notification-agent", "router-agent", "exception-handler"],
      "domains": ["all"],
      "estimatedLatency": "varies",
      "requiresHumanReview": true,
      "humanTouchpointType": "approval"
    },
    {
      "id": "exception-handler",
      "name": "Exception Handling Agent",
      "description": "Handles exceptions, errors, and edge cases in workflows",
      "capabilities": ["error-classification", "retry-logic", "fallback-execution", "human-escalation"],
      "inputs": ["error", "context", "workflow-state"],
      "outputs": ["resolution", "escalation-request", "retry-decision"],
      "commonPartners": ["all"],
      "domains": ["all"],
      "estimatedLatency": "1-5 seconds",
      "requiresHumanReview": false,
      "humanReviewTriggers": ["unrecoverable-error", "repeated-failures", "unknown-error-type"]
    },
    {
      "id": "reconciliation-agent",
      "name": "Reconciliation Agent",
      "description": "Reconciles data across multiple sources and identifies discrepancies",
      "capabilities": ["data-matching", "break-detection", "variance-analysis", "auto-resolution"],
      "inputs": ["dataset-a", "dataset-b", "matching-rules"],
      "outputs": ["matched-records", "breaks", "variance-report"],
      "commonPartners": ["data-validator", "exception-handler", "notification-agent"],
      "domains": ["finance", "trading", "accounting"],
      "estimatedLatency": "5-30 seconds",
      "requiresHumanReview": false,
      "humanReviewTriggers": ["high-value-breaks", "pattern-anomaly"]
    },
    {
      "id": "compliance-checker",
      "name": "Compliance Check Agent",
      "description": "Validates transactions and actions against regulatory requirements",
      "capabilities": ["rule-validation", "regulatory-checking", "policy-enforcement", "audit-logging"],
      "inputs": ["transaction", "entity", "action"],
      "outputs": ["compliance-status", "violations", "audit-record"],
      "commonPartners": ["data-validator", "approval-agent", "notification-agent"],
      "domains": ["finance", "kyc", "trading", "healthcare"],
      "estimatedLatency": "2-5 seconds",
      "requiresHumanReview": false,
      "humanReviewTriggers": ["potential-violation", "unclear-regulation"]
    },
    {
      "id": "report-generator",
      "name": "Report Generation Agent",
      "description": "Generates reports, summaries, and documentation",
      "capabilities": ["report-compilation", "data-visualization", "template-rendering", "pdf-generation"],
      "inputs": ["data", "template", "parameters"],
      "outputs": ["report", "summary", "documentation"],
      "commonPartners": ["enrichment-agent", "notification-agent"],
      "domains": ["all"],
      "estimatedLatency": "5-15 seconds",
      "requiresHumanReview": false
    },
    {
      "id": "election-processor",
      "name": "Election Processing Agent",
      "description": "Processes corporate action elections and entitlements",
      "capabilities": ["election-capture", "entitlement-calculation", "deadline-tracking", "instruction-generation"],
      "inputs": ["election", "position", "corporate-action"],
      "outputs": ["processed-election", "instructions", "confirmation"],
      "commonPartners": ["notification-agent", "compliance-checker", "reconciliation-agent"],
      "domains": ["corporate-actions"],
      "estimatedLatency": "3-10 seconds",
      "requiresHumanReview": false,
      "humanReviewTriggers": ["complex-election", "deadline-risk"]
    },
    {
      "id": "position-keeper",
      "name": "Position Management Agent",
      "description": "Tracks and manages positions, holdings, and balances",
      "capabilities": ["position-tracking", "balance-calculation", "movement-processing", "snapshot-generation"],
      "inputs": ["transactions", "corporate-actions", "transfers"],
      "outputs": ["position-update", "balance", "movement-record"],
      "commonPartners": ["reconciliation-agent", "report-generator", "notification-agent"],
      "domains": ["trading", "corporate-actions", "custody"],
      "estimatedLatency": "1-5 seconds",
      "requiresHumanReview": false
    },
    {
      "id": "kyc-screener",
      "name": "KYC Screening Agent",
      "description": "Screens entities against sanctions lists, PEP databases, and adverse media",
      "capabilities": ["sanctions-screening", "pep-check", "adverse-media-search", "entity-verification"],
      "inputs": ["entity-details", "documents"],
      "outputs": ["screening-result", "risk-score", "matches"],
      "commonPartners": ["document-extractor", "compliance-checker", "approval-agent"],
      "domains": ["kyc", "aml", "onboarding"],
      "estimatedLatency": "5-20 seconds",
      "requiresHumanReview": false,
      "humanReviewTriggers": ["potential-match", "high-risk-score", "incomplete-data"]
    },
    {
      "id": "trade-matcher",
      "name": "Trade Matching Agent",
      "description": "Matches trades between counterparties and internal systems",
      "capabilities": ["trade-matching", "affirmation", "confirmation-generation", "break-resolution"],
      "inputs": ["trade-details", "counterparty-data"],
      "outputs": ["match-status", "confirmation", "breaks"],
      "commonPartners": ["reconciliation-agent", "exception-handler", "notification-agent"],
      "domains": ["trading", "settlement"],
      "estimatedLatency": "2-10 seconds",
      "requiresHumanReview": false,
      "humanReviewTriggers": ["unmatched-trade", "economic-break"]
    },
    {
      "id": "settlement-instructor",
      "name": "Settlement Instruction Agent",
      "description": "Generates and manages settlement instructions",
      "capabilities": ["instruction-generation", "ssi-lookup", "netting", "priority-management"],
      "inputs": ["trade", "settlement-details", "ssi"],
      "outputs": ["settlement-instruction", "status"],
      "commonPartners": ["trade-matcher", "position-keeper", "notification-agent"],
      "domains": ["trading", "settlement"],
      "estimatedLatency": "2-5 seconds",
      "requiresHumanReview": false,
      "humanReviewTriggers": ["missing-ssi", "high-value-settlement"]
    }
  ]
}
